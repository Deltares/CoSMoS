<?xml version="1.0"?>
<root>
  <tile_map name="flood_map">
    <legend_text>Flood depth</legend_text>
    <contour>
      <legend_text> > 2.0 m</legend_text>
      <lower type="float">2.0</lower>
      <upper type="float">999999.0</upper>
      <rgb type="int">255, 0, 0</rgb>
    </contour>
    <contour>
      <legend_text>1.0 - 2.0 m</legend_text>
      <lower type="float">1.0</lower>
      <upper type="float">2.0</upper>
      <rgb type="int">255, 165, 0</rgb>
    </contour>
    <contour>
      <legend_text>0.3 - 1.0 m</legend_text>
      <lower type="float">0.3</lower>
      <upper type="float">1.0</upper>
      <rgb type="int">255, 255, 0</rgb>
    </contour>
    <contour>
      <legend_text>0.0 - 0.3 m</legend_text>
      <lower type="float">0.1</lower>
      <upper type="float">0.3</upper>
      <rgb type="int">0, 255, 0</rgb>
    </contour>
  </tile_map>
  <tile_map name="run_up">
    <legend_text>Run-up above MSL</legend_text>
    <contour>
      <legend_text> > 4.0 m</legend_text>
      <lower type="float">4.0</lower>
      <upper type="float">999999.0</upper>
      <rgb type="int">255, 0, 0</rgb>
    </contour>
    <contour>
      <legend_text>2.0 - 4.0 m</legend_text>
      <lower type="float">2.0</lower>
      <upper type="float">4.0</upper>
      <rgb type="int">255, 165, 0</rgb>
    </contour>
    <contour>
      <legend_text>1.0 - 2.0 m</legend_text>
      <lower type="float">1.0</lower>
      <upper type="float">2.0</upper>
      <rgb type="int">255, 255, 0</rgb>
    </contour>
    <contour>
      <legend_text>0.0 - 1.0 m</legend_text>
      <lower type="float">0.0</lower>
      <upper type="float">1.0</upper>
      <rgb type="int">0, 255, 0</rgb>
    </contour>
  </tile_map>
  <tile_map name="sedero">
    <legend_text>Sedimentation/erosion</legend_text>
    <contour>
      <legend_text>&lt; -1.5 m</legend_text>
      <lower type="float">-99999.0</lower>
      <upper type="float">-1.5</upper>
      <rgb type="int">103, 0, 31</rgb>
    </contour>
    <contour>
      <legend_text>-1.5 - -1 m</legend_text>
      <lower type="float">-1.5</lower>
      <upper type="float">-1.0</upper>
      <rgb type="int">187, 42, 51</rgb>
    </contour>
    <contour>
      <legend_text>-1.0 - -0.5 m</legend_text>
      <lower type="float">-1.0</lower>
      <upper type="float">-0.5</upper>
      <rgb type="int">229, 131, 104</rgb>
    </contour>
    <contour>
      <legend_text>-0.5 - -0.25 m</legend_text>
      <lower type="float">-0.5</lower>
      <upper type="float">-0.25</upper>
      <rgb type="int">250, 206, 182</rgb>
    </contour>
    <contour>
      <legend_text>0.25 - 0.5 m</legend_text>
      <lower type="float">0.25</lower>
      <upper type="float">0.5</upper>
      <rgb type="int">191, 220, 235</rgb>
    </contour>
    <contour>
      <legend_text>0.5 - 1.0 m</legend_text>
      <lower type="float">0.5</lower>
      <upper type="float">1.0</upper>
      <rgb type="int">104, 170, 207</rgb>
    </contour>
    <contour>
      <legend_text>1.0 - 1.5 m</legend_text>
      <lower type="float">1.0</lower>
      <upper type="float">1.5</upper>
      <rgb type="int">40, 111, 176</rgb>
    </contour>
    <contour>
      <legend_text>&gt;= 1.5</legend_text>
      <lower type="float">1.5</lower>
      <upper type="float">999999.9</upper>
      <rgb type="int">5, 48, 97</rgb>
    </contour>
  </tile_map>
  <tile_map name="Hm0_2m">
    <legend_text>Wave height (m)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">0.25</lower>
    <upper type="float">2.0</upper>
    <step type="float">0.25</step>
  </tile_map>
  <tile_map name="Hm0_5m">
    <legend_text>Wave height (m)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">0.5</lower>
    <upper type="float">5.0</upper>
    <step type="float">0.5</step>
  </tile_map>
  <tile_map name="Hm0_10m">
    <legend_text>Wave height (m)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">1.0</lower>
    <upper type="float">10.0</upper>
    <step type="float">1.0</step>
  </tile_map>
  <tile_map name="Hm0_20m">
    <legend_text>Wave height (m)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">2.0</lower>
    <upper type="float">20.0</upper>
    <step type="float">2.0</step>
  </tile_map>
  <tile_map name="bed_levels">
    <legend_text>Bed level (m)</legend_text>
    <color_map>GMT_globe</color_map>
    <lower type="float">-10.0</lower>
    <upper type="float">10.0</upper>
    <step type="float">1.0</step>
  </tile_map>  
  <tile_map name="wnd20">
    <legend_text>Wind speed (m/s)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">2.0</lower>
    <upper type="float">20.0</upper>
    <step type="float">2.0</step>
  </tile_map>
  <tile_map name="wnd40">
    <legend_text>Wind speed (m/s)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">5.0</lower>
    <upper type="float">40.0</upper>
    <step type="float">5.0</step>
  </tile_map>
  <tile_map name="wnd60">
    <legend_text>Wind speed (m/s)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">5.0</lower>
    <upper type="float">60.0</upper>
    <step type="float">5.0</step>
  </tile_map>
  <tile_map name="precip0050">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">5.0</lower>
    <upper type="float">50.0</upper>
    <step type="float">5.0</step>
  </tile_map>
  <tile_map name="precip0100">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">10.0</lower>
    <upper type="float">100.0</upper>
    <step type="float">10.0</step>
  </tile_map>
  <tile_map name="precip0200">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">20.0</lower>
    <upper type="float">200.0</upper>
    <step type="float">20.0</step>
  </tile_map>
  <tile_map name="precip0500">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">50.0</lower>
    <upper type="float">500.0</upper>
    <step type="float">50.0</step>
  </tile_map>
  <tile_map name="precip1000">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">100.0</lower>
    <upper type="float">1000.0</upper>
    <step type="float">100.0</step>
  </tile_map>
  <tile_map name="precip_log">
    <legend_text>Rainfall (mm)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <scale>log16</scale>
    <lower type="float">1.0</lower>
    <upper type="float">1000.0</upper>
  </tile_map>
  <tile_map name="Hm0">
    <legend_text>Wave height (m)</legend_text>
    <color_map>cyan_purple_jet</color_map>
    <lower type="float">1.0</lower>
    <upper type="float">26.0</upper>
    <scale>log121</scale>    
  </tile_map>
</root>
